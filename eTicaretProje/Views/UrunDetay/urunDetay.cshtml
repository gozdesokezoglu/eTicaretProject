@functions{
    public string GetString(string tag, int indis)
    {
        string result;
        switch (indis)
        {
            case 1:
                result = tag + "One";
                break;
            case 2:
                result = tag + "Two";
                break;
            case 3:
                result = tag + "Three";
                break;
            case 4:
                result = tag + "Four";
                break;
            case 5:
                result = tag + "Five";
                break;
            case 6:
                result = tag + "Six";
                break;
            case 7:
                result = tag + "Seven";
                break;
            case 8:
                result = tag + "Eight";
                break;
            case 9:
                result = tag + "Nine";
                break;
            case 10:
                result = tag + "Ten";
                break;
            default:
                result = tag;
                break;
        }
        return result;
    }
}
